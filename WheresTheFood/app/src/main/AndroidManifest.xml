<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mob.dev.wheresthefood.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mob.dev.wheresthefood.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mob.dev.wheresthefood.app.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="mob.dev.wheresthefood.app.SignUp"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="mob.dev.wheresthefood.app.ForgottenPass"
            android:label="@string/title_activity_forgotten_pass" >
        </activity>
        <activity
            android:name="mob.dev.wheresthefood.app.TODOList"
            android:label="@string/title_activity_todolist" >
        </activity>
        <activity
            android:name="mob.dev.wheresthefood.app.ShoppingList"
            android:label="@string/title_activity_shopping_list" >
        </activity>
    </application>

</manifest>
